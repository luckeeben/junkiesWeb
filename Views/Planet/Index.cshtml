@using junkiesWeb.Models
@model List<Planet>
@{
    ViewBag.Title = "Planets";
}
<h1>@ViewBag.Title</h1>


<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
		<th>Sector Id</th>
    </tr>


@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
		<td>
            @Html.DisplayFor(modelItem => item.SectorId)
        </td>
    </tr>
}

</table>